# Anthropic (Claude API)
ANTHROPIC_API_KEY=your-anthropic-api-key

# Azure
AZURE_SUBSCRIPTION_ID=your-azure-subscription-id
AZURE_TENANT_ID=your-azure-tenant-id
AZURE_ML_WORKSPACE_NAME=countyiq-workspace
AZURE_RESOURCE_GROUP=countyiq-rg

# Azure OpenAI
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_KEY=your-azure-openai-key

# Azure AI Search (vector store)
AZURE_SEARCH_ENDPOINT=https://your-search.search.windows.net
AZURE_SEARCH_KEY=your-azure-search-key

# Azure Data Lake Storage Gen2
ADLS_ACCOUNT_NAME=countyiqdata
ADLS_ACCOUNT_KEY=your-adls-account-key
ADLS_CONNECTION_STRING=your-adls-connection-string

# DuckDB local database
DUCKDB_PATH=F:/countyiq/db/countyiq.duckdb
